pub func void DrawImage(imageName, posX, posY, width, height)
{
	ASM
	{
		LITERAL imageName
		LITERAL height
		LITERAL width
		LITERAL posY
		LITERAL posX
		DRAW_IMAGE
	}
}

pub func void DrawCookie() {
	DrawImage("cookie.png", 0, 0, 64, 64);
}

